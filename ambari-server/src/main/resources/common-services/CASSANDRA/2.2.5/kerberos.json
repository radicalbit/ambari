{
  "services": [
    {
      "name": "CASSANDRA",
      "identities": [
        {
          "name": "/smokeuser"
        }
      ],
      "configurations": [
        {
          "cassandra-env": {
            "qop": "auth"
          }
        }
      ],
      "components": [
        {
          "name": "CASSANDRA_SEED",
          "identities": [
            {
              "name": "cassandra_seed_cs",
              "principal": {
                "value": "cs/_HOST@${realm}",
                "type" : "service",
                "configuration": "cassandra-env/cassandra_seed_principal_name",
                "local_username" : "${cassandra-env/cassandra_user}"
              },
              "keytab": {
                "file": "${keytab_dir}/cs.service.keytab",
                "owner": {
                  "name": "${cassandra-env/cassandra_user}",
                  "access": "r"
                },
                "group": {
                  "name": "${cluster-env/user_group}",
                  "access": ""
                },
                "configuration": "cassandra-env/cassandra_seed_keytab"
              }
            }
          ]
        },
        {
          "name": "CASSANDRA_NODE",
          "identities": [
            {
              "name": "cassandra_node_cn",
              "principal": {
                "value": "cn/_HOST@${realm}",
                "type" : "service",
                "configuration": "cassandra-env/cassandra_node_principal_name",
                "local_username" : "${cassandra-env/cassandra_user}"
              },
              "keytab": {
                "file": "${keytab_dir}/cn.service.keytab",
                "owner": {
                  "name": "${cassandra-env/cassandra_user}",
                  "access": "r"
                },
                "group": {
                  "name": "${cluster-env/user_group}",
                  "access": ""
                },
                "configuration": "cassandra-env/cassandra_node_keytab"
              }
            }
          ]
        }
      ]
    }
  ]
}