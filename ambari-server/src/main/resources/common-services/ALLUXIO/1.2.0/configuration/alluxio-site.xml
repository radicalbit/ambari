<configuration>

  <!--<property require-input="true">-->
    <!--<name>alluxio.worker.evictor.class</name>-->
    <!--<description>Alluxio Worker Evictor Class</description>-->
    <!--<value>alluxio.worker.block.evictor.LRUEvictor</value>-->
  <!--</property>-->

  <property require-input="true">
    <name>alluxio.worker.memory</name>
    <description>Alluxio Worker Memory</description>
    <value>2</value>
    <value-attributes>
      <type>int</type>
      <unit>GB</unit>
    </value-attributes>
  </property>

  <!--<property require-input="true">-->
    <!--<name>alluxio.worker.tieredstore.levels</name>-->
    <!--<description>Alluxio Worker Tieredstore Levels</description>-->
    <!--<value>2</value>-->
  <!--</property>-->

  <property require-input="true">
    <name>alluxio.worker.tieredstore.level0.alias</name>
    <description>Alluxio Worker Tieredstore Level0 Alias</description>
    <value>MEM</value>
    <value-attributes>
      <type>value-list</type>
      <entries>
        <entry>
          <value>MEM</value>
          <label>MEM</label>
        </entry>
        <entry>
          <value>SSD</value>
          <label>SSD</label>
        </entry>
        <entry>
          <value>HDD</value>
          <label>HDD</label>
        </entry>
      </entries>
      <selection-cardinality>1</selection-cardinality>
    </value-attributes>
  </property>

  <property require-input="true">
    <name>alluxio.worker.tieredstore.level0.dirs.path</name>
    <description>Alluxio Worker Tieredstore Level0 Dirs Path</description>
    <value>/mnt/ramdisk</value>
    <value-attributes>
      <type>directory</type>
    </value-attributes>
  </property>

  <property require-input="true">
    <name>alluxio.worker.tieredstore.level0.dirs.quota</name>
    <description>Alluxio Worker Tieredstore Level0 Dirs Quota</description>
    <value>${alluxio.worker.memory.size}</value>
    <value-attributes>
      <unit>GB</unit>
    </value-attributes>
  </property>

  <property require-input="true">
    <name>alluxio.worker.tieredstore.level0.reserved.ratio</name>
    <description>Alluxio Worker Tieredstore Level0 Reserved Ratio</description>
    <value>0.1</value>
  </property>

  <property require-input="false">
    <name>alluxio.worker.tieredstore.level1.alias</name>
    <description>Alluxio Worker Tieredstore Level1 Alias</description>
    <value>SSD</value>
    <value-attributes>
      <type>value-list</type>
      <entries>
        <entry>
          <value>MEM</value>
          <label>MEM</label>
        </entry>
        <entry>
          <value>SSD</value>
          <label>SSD</label>
        </entry>
        <entry>
          <value>HDD</value>
          <label>HDD</label>
        </entry>
      </entries>
      <selection-cardinality>1</selection-cardinality>
    </value-attributes>
  </property>

  <property require-input="false">
    <name>alluxio.worker.tieredstore.level1.dirs.path</name>
    <description>Alluxio Worker Tieredstore Level1 Dirs Path</description>
    <value>/var/lib/alluxio/level1_tired_storage</value>
    <value-attributes>
      <type>directory</type>
    </value-attributes>
  </property>

  <property require-input="false">
    <name>alluxio.worker.tieredstore.level1.dirs.quota</name>
    <description>Alluxio Worker Tieredstore Level1 Dirs Quota</description>
    <value>10</value>
    <value-attributes>
      <type>int</type>
      <unit>GB</unit>
    </value-attributes>
  </property>

  <property require-input="false">
    <name>alluxio.worker.tieredstore.level1.reserved.ratio</name>
    <description>Alluxio Worker Tieredstore Level1 Reserved Ratio</description>
    <value>0.1</value>
  </property>

  <property require-input="false">
    <name>alluxio.worker.tieredstore.level2.alias</name>
    <description>Alluxio Worker Tieredstore Level2 Alias</description>
    <value></value>
    <value-attributes>
      <empty-value-valid>true</empty-value-valid>
      <type>value-list</type>
      <entries>
        <entry>
          <value>MEM</value>
          <label>MEM</label>
        </entry>
        <entry>
          <value>SSD</value>
          <label>SSD</label>
        </entry>
        <entry>
          <value>HDD</value>
          <label>HDD</label>
        </entry>
      </entries>
      <selection-cardinality>1</selection-cardinality>
    </value-attributes>
  </property>

  <property require-input="false">
    <name>alluxio.worker.tieredstore.level2.dirs.path</name>
    <description>Alluxio Worker Tieredstore Level2 Dirs Path</description>
    <value></value>
    <value-attributes>
      <empty-value-valid>true</empty-value-valid>
      <type>directory</type>
    </value-attributes>
  </property>

  <property require-input="false">
    <name>alluxio.worker.tieredstore.level2.dirs.quota</name>
    <description>Alluxio Worker Tieredstore Level2 Dirs Quota</description>
    <value></value>
    <value-attributes>
      <empty-value-valid>true</empty-value-valid>
      <type>int</type>
      <unit>GB</unit>
    </value-attributes>
  </property>

  <property require-input="false">
    <name>alluxio.worker.tieredstore.level2.reserved.ratio</name>
    <description>Alluxio Worker Tieredstore Level2 Reserved Ratio</description>
    <value></value>
    <value-attributes>
      <empty-value-valid>true</empty-value-valid>
    </value-attributes>
  </property>

  <!--<property require-input="true">-->
    <!--<name>alluxio.worker.tieredstore.reserver.enabled</name>-->
    <!--<description>Alluxio Worker Tieredstore Reserver Enabled</description>-->
    <!--<value>false</value>-->
  <!--</property>-->

  <property require-input="true">
    <name>alluxio.worker.web.port</name>
    <description>Alluxio Worker Web Port</description>
    <value>30000</value>
    <value-attributes>
      <type>int</type>
    </value-attributes>
  </property>

  <property require-input="true">
    <name>alluxio.master.web.port</name>
    <description>Alluxio Master Web Port</description>
    <value>19999</value>
    <value-attributes>
      <type>int</type>
    </value-attributes>
  </property>

  <property require-input="true">
    <name>alluxio.user.file.readtype.default</name>
    <description>Alluxio User File Readtype Default</description>
    <value>CACHE_PROMOTE</value>
    <value-attributes>
      <type>value-list</type>
      <entries>
        <entry>
          <value>CACHE_PROMOTE</value>
          <label>CACHE_PROMOTE</label>
        </entry>
        <entry>
          <value>CACHE</value>
          <label>CACHE</label>
        </entry>
        <entry>
          <value>NO_CACHE</value>
          <label>NO_CACHE</label>
        </entry>
      </entries>
      <selection-cardinality>1</selection-cardinality>
    </value-attributes>
  </property>

  <property require-input="true">
    <name>alluxio.user.block.size.bytes.default</name>
    <description>Default block size for Alluxio files.</description>
    <value>128</value>
    <value-attributes>
      <type>int</type>
      <unit>MB</unit>
    </value-attributes>
  </property>

  <!--<property require-input="true">-->
    <!--<name>alluxio.user.file.write.location.policy.class</name>-->
    <!--<description>Alluxio User File Write Location Policy Class</description>-->
    <!--<value>alluxio.client.file.policy.LocalFirstPolicy</value>-->
  <!--</property>-->

  <property require-input="true">
    <name>alluxio.user.file.writetype.default</name>
    <description>Alluxio User File Writetype Default</description>
    <value>MUST_CACHE</value>
    <value-attributes>
      <type>value-list</type>
      <entries>
        <entry>
          <value>MUST_CACHE</value>
          <label>MUST_CACHE</label>
        </entry>
        <entry>
          <value>CACHE_THROUGH</value>
          <label>CACHE_THROUGH</label>
        </entry>
        <entry>
          <value>THROUGH</value>
          <label>THROUGH</label>
        </entry>
      </entries>
      <selection-cardinality>1</selection-cardinality>
    </value-attributes>
  </property>

